<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>WebsistemakGYM</display-name>
  <welcome-file-list>
    <welcome-file>/html/Welcome.html</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>5</session-timeout>
  </session-config>
  <listener>
    <listener-class>GYMWeb.SessionListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>SignupServlet</servlet-name>
    <servlet-class>GYMWeb.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/servlet/SignupServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>GYMWeb.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>GYMWeb.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/servlet/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>OptionServlet</servlet-name>
    <servlet-class>GYMWeb.OptionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OptionServlet</servlet-name>
    <url-pattern>/servlet/OptionServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShowInfoServlet</servlet-name>
    <servlet-class>GYMWeb.ShowInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowInfoServlet</servlet-name>
    <url-pattern>/servlet/ShowInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditInfoServlet</servlet-name>
    <servlet-class>GYMWeb.EditInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditInfoServlet</servlet-name>
    <url-pattern>/servlet/EditInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChangePasswordServlet</servlet-name>
    <servlet-class>GYMWeb.ChangePasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChangePasswordServlet</servlet-name>
    <url-pattern>/servlet/ChangePasswordServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RoutineToPDFServlet</servlet-name>
    <servlet-class>GYMWeb.RoutineToPDFServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoutineToPDFServlet</servlet-name>
    <url-pattern>/servlet/RoutineToPDFServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InfoToPDFServlet</servlet-name>
    <servlet-class>GYMWeb.InfoToPDFServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InfoToPDFServlet</servlet-name>
    <url-pattern>/servlet/InfoToPDFServlet</url-pattern>
  </servlet-mapping>
</web-app>